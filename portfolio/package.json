{
  "name": "tkr-portfolio",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && cd ../terminal/terminal-app && npm run build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build && cd ../terminal/terminal-app && npm run build && cd ../../portfolio && gh-pages -d public -b master -m \"Deploying to master branch\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "help": "gh-pages --help"
  },
  "dependencies": {
    "gatsby": "^2.13.57",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-remark": "^2.6.13",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-waypoint": "^9.0.2"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "prettier": "1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krushiraj/krushiraj.github.io"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
